{
  "immed": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "trailing": true,
  "maxlen": 200,
  "boss": true,
  "eqnull": true,
  "expr": true,
  "globalstrict": true,
  "laxbreak": true,
  "loopfunc": true,
  "sub": true,
  "undef": true,
  "browser": true,
  "globals": {
