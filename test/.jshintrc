    "angular"    : false,
    "inject"     : false,
    "_jQuery"    : false,
    "browserTrigger": false,

    "it"         : false,
    "iit"        : false,
    "xit"        : false,
    "describe"   : false,
    "ddescribe"  : false,
    "xdescribe"  : false,
    "dump"      : false,
